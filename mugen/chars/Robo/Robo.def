
; Player information
[Info]
name = "Robo"        ;Name of character
displayname = "Robo" ;Name of character to display
versiondate = 12/11/2012    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0          ;Version of M.U.G.E.N character works on (X.XX)
author = "SilentProtagonist"         ;Character author name
pal.defaults = 1,2,3,4,5,6
localcoord = 320,240        ;Local coordinate space width and height

; Files for the player
[Files]
cmd     = Robo.cmd           ;Command set
cns     = Robo.cns           ;Constants
stcommon = common1.cns      ;Common states (from data/ or motif)
sprite  = Robo.sff           ;Sprite
anim    = Robo.air           ;Animation
sound   = Robo.snd           ;Sound
st      = Robo.cns           ;States
st1     = States/System.st
st2     = States/Normal.st 
st3     = States/Specials.st
st4     = States/Supers.st
st5     = States/Helpers.st

; Maps character selection buttons to palette numbers.
[Palette Keymap]
a = 1
b = 2
c = 3
x = 4
y = 5
z = 6
x2 = 7 
y2 = 8
z2 = 9
;a2 = 10
;b2 = 11
;c2 = 12


